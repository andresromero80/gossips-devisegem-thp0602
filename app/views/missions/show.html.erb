<h1>Missions#show</h1>
<p>Find me in app/views/missions/show.html.erb</p>

<div class="card">
  <div class="card-header">
    <h3><%= @mission.title %></h3>
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p><%= @mission.title %></p>
      <footer class="blockquote-footer"> by: <%= @mission.user_id %> <cite title="Source Title">on <%= @mission.created_at %></cite></footer>
    </blockquote>
  </div>
</div>